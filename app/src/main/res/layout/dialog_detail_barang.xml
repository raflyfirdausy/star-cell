<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#ffff">
        <ImageView
            android:id="@+id/iv_gambarBarang"
            android:layout_width="200dp"
            android:layout_height="150dp"
            android:layout_gravity="center"
            android:src="@drawable/bg_take_pict" />
        <LinearLayout
            android:layout_width="400dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="30dp"
            android:layout_marginEnd="30dp"
            android:background="#CEFFFFFF"
            android:minHeight="500dp">


            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="20dp">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <com.rfl.trn.starr_cell.Custom.MyTextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textAlignment="center"
                        android:text="Detail Barang"
                        android:textColor="#050505"
                        android:layout_marginTop="30dp"
                        android:textSize="28sp"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="30dp" />

                    <com.rfl.trn.starr_cell.Custom.MyTextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginEnd="20dp"
                        android:textColor="#050505"
                        android:text="Nama Barang"
                        android:textSize="18sp" />

                    <com.rfl.trn.starr_cell.Custom.MyTextView
                        android:id="@+id/myet_dialogNamaBarang"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginEnd="20dp"
                        android:textColor="#050505"
                        android:layout_marginBottom="10dp"
                        android:text="Pulsa Perdana"
                        android:textSize="23sp" />

                    <com.rfl.trn.starr_cell.Custom.MyTextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginEnd="20dp"
                        android:text="Konter"
                        android:textSize="18sp" />

                    <com.rfl.trn.starr_cell.Custom.MyTextView
                        android:id="@+id/myet_dialogKonterBarang"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginEnd="20dp"
                        android:textColor="#050505"
                        android:layout_marginBottom="10dp"
                        android:text="Star Cell 100"
                        android:textSize="23sp" />

                    <com.rfl.trn.starr_cell.Custom.MyTextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginEnd="20dp"
                        android:textColor="#050505"
                        android:text="Kategori"
                        android:textSize="18sp" />

                    <com.rfl.trn.starr_cell.Custom.MyTextView
                        android:id="@+id/myet_dialogKategoriBarang"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginEnd="20dp"
                        android:layout_marginBottom="10dp"
                        android:textColor="#050505"
                        android:text="Perdana"
                        android:textSize="23sp" />

                    <com.rfl.trn.starr_cell.Custom.MyTextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginEnd="20dp"
                        android:textColor="#050505"
                        android:text="Stok Barang"
                        android:textSize="18sp" />

                    <com.rfl.trn.starr_cell.Custom.MyTextView
                        android:id="@+id/myet_dialogStokBarang"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginEnd="20dp"
                        android:layout_marginBottom="10dp"
                        android:textColor="#050505"
                        android:text="Tersedia 500"
                        android:textSize="23sp" />

                    <com.rfl.trn.starr_cell.Custom.MyTextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginEnd="20dp"
                        android:textColor="#050505"
                        android:text="Harga"
                        android:textSize="18sp" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginEnd="20dp"
                        android:textColor="#050505"
                        android:orientation="horizontal">

                        <com.rfl.trn.starr_cell.Custom.MyTextView
                            android:id="@+id/myet_dialogHarga1Barang"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="3dp"
                            android:layout_marginEnd="3dp"
                            android:layout_marginBottom="10dp"
                            android:textColor="#050505"
                            android:layout_weight="1"
                            android:text="Rp 00000"
                            android:textSize="18sp" />

                        <com.rfl.trn.starr_cell.Custom.MyTextView
                            android:id="@+id/myet_dialogHarga2Barang"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="3dp"
                            android:layout_marginEnd="3dp"
                            android:layout_marginBottom="10dp"
                            android:textColor="#050505"
                            android:layout_weight="1"
                            android:text="Rp 000000"
                            android:textSize="18sp" />

                        <com.rfl.trn.starr_cell.Custom.MyTextView
                            android:id="@+id/myet_dialogHarga3Barang"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="3dp"
                            android:layout_marginEnd="3dp"
                            android:layout_marginBottom="10dp"
                            android:textColor="#050505"
                            android:layout_weight="1"
                            android:text="Rp 000000"
                            android:textSize="18sp" />
                    </LinearLayout>

                    <com.rfl.trn.starr_cell.Custom.MyTextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginEnd="20dp"
                        android:text="Terakhir Diubah"
                        android:textColor="#050505"
                        android:textSize="18sp" />

                    <com.rfl.trn.starr_cell.Custom.MyTextView
                        android:id="@+id/myet_dialogTanggalBarang"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginEnd="20dp"
                        android:layout_marginBottom="10dp"
                        android:textColor="#050505"
                        android:text="12/13/200"
                        android:textSize="23sp" />

                </LinearLayout>
            </ScrollView>
        </LinearLayout>
    </FrameLayout>


</LinearLayout>