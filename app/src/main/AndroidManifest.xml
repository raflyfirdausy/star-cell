<?xml version="1.0" encoding="utf-8"?><!--suppress ALL -->
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tool="http://schemas.android.com/tools"
    package="com.rfl.trn.starr_cell">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />
    <uses-feature android:name="android.hardware.camera.autofocus" />
    <uses-feature android:name="android.hardware.camera.flash" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_sell"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_sell"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tool:ignore="GoogleAppIndexingWarning"
        tool:replace="android:icon">

        <!--suppress AndroidDomInspection -->
        <activity
            android:name=".Fragment.Admin.AdminTransaksiFragment"
            tool:ignore="InnerclassSeparator" />
        <!--suppress AndroidDomInspection -->
        <activity
            android:name=".Fragment.Karyawan.KaryawanAbsensiFragment"
            tool:ignore="InnerclassSeparator" />
        <activity
            android:name=".Fragment.Admin.AdminRekapanAbsensiFragment"
            tool:ignore="InnerclassSeparator" />
        <activity
            android:name=".Fragment.Admin.AdminAbsensiFragment"
            tool:ignore="InnerclassSeparator" />
        <activity
            android:name=".Activity.CetakStrukActivity"
            tool:ignore="InnerclassSeparator" />
        <activity
            android:name=".Activity.CekAbsensiActivity"
            tool:ignore="InnerclassSeparator" />
        <activity
            android:name=".Activity.DetailTransaksiActivity"
            tool:ignore="InnerclassSeparator" />
        <activity
            android:name=".Activity.TambahBarangActivity"
            tool:ignore="InnerclassSeparator" />
        <activity
            android:name=".Activity.TambahKaryawanActivity"
            tool:ignore="InnerclassSeparator" />
        <activity
            android:name=".Activity.DaftarKonterActivity"
            android:screenOrientation="portrait"
            tool:ignore="InnerclassSeparator" />
        <activity
            android:name="com.yalantis.ucrop.UCropActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".Activity.LoginActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBarTransparent"
            tool:ignore="InnerclassSeparator" />
        <activity
            android:name=".Activity.MainActivity"
            android:label="@string/title_activity_dashboard"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            tool:ignore="InnerclassSeparator" />
        <activity
            android:name=".Activity.SplashActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBarTransparent"
            tool:ignore="InnerclassSeparator">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>